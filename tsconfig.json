{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types"],
    "lib": ["es2022"],
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "sourceMap": true,
    "target": "es2022",
    "esModuleInterop": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "declaration": true,
    "moduleDetection": "force",
    "allowJs": true,
    "resolveJsonModule": true
  }
}
          